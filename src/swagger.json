{
  "openapi": "3.0.0",
  "info": {
    "title": "Sos Dog - Documention",
    "description": "1.0.0",
    "contact": {
      "email": "sos_dog@gmail.com"
    }
  },
  "paths": {
    "/users": {
      "post": {
        "tags": [
          "Users"
        ],
        "summary": "Create a user",
        "description": "Create a new user",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "email": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  },
                  "username": {
                    "type": "string"
                  },
                  "type_user": {
                    "type": "string"
                  }
                },
                "example": {
                  "name": "testName",
                  "email": "testEmail@test.com",
                  "password": "1234",
                  "username": "testeUsername",
                  "type_user": "teste"
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created"
          },
          "500": {
            "description": "Username already exists"
          },
          "500": {
            "description": "Email already exists"
          }
        }
      },
      "get": {
        "tags": [
          "Users"
        ],
        "summary": "List all users",
        "description": "List all users",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "name": {
                        "type": "string"
                      },
                      "email": {
                        "type": "string"
                      },
                      "password": {
                        "type": "string"
                      },
                      "username": {
                        "type": "string"
                      },
                      "type_user": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    
    
  },
  "/sessions": {
    "post": {
      "tags": [
        "Users"
      ],
      "summary": "Authenticate user",
      "description": "Authenticate user and generate an access token",
      "requestBody": {
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "email": {
                  "type": "string"
                },
                "password": {
                  "type": "string"
                }
              },
              "example": {
                "email": "admin@rentx.com",
                "password": "admin"
              }
            }
          }
        }
      },
      "responses": {
        "200": {
          "description": "Authenticated"
        },
        "400": {
          "description": "Email or password incorrect"
        }
      }
    }
  }
  },
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  }
}